<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All of Your Entries</title>
</head>
<body>
    <h1>Your Journey</h1>
    <% logs.forEach((log) => { %>
        <li>
            <a href="/logs/<%= log._id %>">
                <%= log.title %></a>
                <p><%= log.date %></p>
                <a href="/logs/<%= log._id %>/edit">Edit</a>
            <form action="/logs/<%= log._id %>?_method=DELETE" method="POST">
                <button>Delete</button>
        
            </form>
        </li>
    <% }) %>
</body>
</html>